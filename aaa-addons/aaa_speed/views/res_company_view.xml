<odoo>
    <record id="view_company_speed" model="ir.ui.view">
        <field name="name">res.company.form.spped.inherit</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
           <xpath expr="//field[@name='parent_id']" position="before">
                <field name="manage_speed" invisible="True"/>
           </xpath>
        </field>
    </record>
</odoo>
