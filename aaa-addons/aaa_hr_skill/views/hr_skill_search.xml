<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_skill_search_form" model="ir.ui.view">
        <field name="name">hr.skill.search.form</field>
        <field name="model">hr.skill.search</field>
        <field name="arch" type="xml">
            <form string="Compétences recherchées">
                <sheet>
                    <label for="complete_name" class="oe_edit_only"/>
                    <h1><field name="skill_id"/></h1>
                    <group>
                        <group>
                            <field name="description"/>
                            <field name="level_required"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
