<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
        <t t-name="button_for_company">
            <div class="search-range_company btn-group-sm" >
            <div style="padding-right: 10px;">
                <span style="display: inline-block;">
                       <span style="font-weight: bold;">Cabinet :</span>
                </span>
                 <span style="display: inline-block; margin-left: 2px;">
                        <select id="id_input_field_company" style="margin-left:35px;width:210px;" class="input_field_company form-control">
                            <option t-foreach="values_field" t-as="value" t-att-value="value[0]">
                            <t t-esc="value[1]"/>
                            </option>
                        </select>
                </span>
            </div>
        </div>
 </t>

