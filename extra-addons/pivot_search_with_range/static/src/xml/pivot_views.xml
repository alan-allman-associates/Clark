<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
        <t t-name="button_for_company">
            <div class="search-range_company">
            <div class="row">
                <div class="col-md-4">
                    <span>
                           <span style="font-weight: bold;">Cabinet :</span>
                    </span>
                </div>
                <div class="col-md-8">
                     <span>
                            <select id="id_input_field_company" style="width: 255px;" class="input_field_company form-control">
                                <option t-foreach="values_field" t-as="value" t-att-value="value[0]">
                                <t t-esc="value[1]"/>
                                </option>
                            </select>
                    </span>
                </div>
            </div>
        </div>
 </t>

