	<!--for date-->
 <t t-name="buttons_for_date">
     <div class="search-range" >
         <div class="row">
             <div class="col-md-4">
                 <span style="float: left;">
                     <span style="font-weight: bold;">Date du clôture:</span>
                 </span>
             </div>
             <div class="col-md-7" style="width:200px;">
                 <div class="row">
                     <div class="col-md-6">
                         <span>
                             <input id="field_start_date" type="text" name="field_start_date" size="10" class="field_start_date form-control" data-toggle="datetimepicker" data-target="#field_start_date" style="height: 30px;text-align: center;"/>
                         </span>
                     </div>
                     <div class="col-md-6">
                         <span>
                             <input id="field_end_date" type="text" name="field_end_date" size="10" class="field_end_date form-control" data-toggle="datetimepicker" data-target="#field_end_date" style="height: 30px;text-align: center;"/>
                         </span>
                     </div>
                 </div>
             </div>
         </div>
     </div>
 </t>


